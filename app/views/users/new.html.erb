<h1>Signup</h1>
<p><%= flash[:notice] %><p>
<br>
<div>
<%= form_for @user do |f| %>
  <p><%= f.label :email %>:
  <%= f.text_field :email %>
  </p>
  <p><%= f.label :password %>:
  <%= f.password_field :password %>
  </p>
  <p>
  <%= f.label :password_confirmation %>:
  <%= f.password_field :password_confirmation %>
  </p>
  <%= f.submit 'Register' %>
<% end %>
</div>